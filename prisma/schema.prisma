// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Jobs {
  id      String   @id @default(cuid())
  date    DateTime
  title   String
  company String
  salary  String?
  url     String
  logo    String?
}
